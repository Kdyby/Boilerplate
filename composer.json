        "php":">=5.3.2",
        "nette/nette":"dev-devel",
        "doctrine/orm":"2.2.*",
        "doctrine/data-fixtures":"v1.0.0-ALPHA2",
        "symfony/console":"2.0.*",
        "symfony/css-selector":"2.0.*",
        "symfony/validator":"2.0.*",
